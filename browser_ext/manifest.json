{
  "manifest_version": 3,
  "name": "My Extension",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "images/ico_16.png"
  },

  "icons": {
    "16": "images/ico_16.png"
  },

  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["<all_urls>"]
    }
  ]
}